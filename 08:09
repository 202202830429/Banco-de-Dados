-- Geração de Modelo físico
-- Sql ANSI 2003 - brModelo.



CREATE TABLE ENGENHEIRO (
CREA VARCHAR(10) PRIMARY KEY,
Nome VARCHAR(10),
Sexo VARCHAR(10),
Especialidade VARCHAR(10)
)

CREATE TABLE PROJETO (
Código VARCHAR(10) PRIMARY KEY,
Nome VARCHAR(10),
Local VARCHAR(10)
)

CREATE TABLE ALUNO (
Nome VARCHAR(10),
Matrícula VARCHAR(10) PRIMARY KEY,
CPF VARCHAR(10),
Curso VARCHAR(10)
)

CREATE TABLE DISCIPLINA (
Nome VARCHAR(10),
Código VARCHAR(10) PRIMARY KEY,
Área VARCHAR(10)
)

CREATE TABLE Trabalhar (
Código VARCHAR(10),
CREA VARCHAR(10),
FOREIGN KEY(Código) REFERENCES PROJETO (Código),
FOREIGN KEY(CREA) REFERENCES ENGENHEIRO (CREA)
)

CREATE TABLE Estudar (
Código VARCHAR(10),
Matrícula VARCHAR(10),
FOREIGN KEY(Código) REFERENCES DISCIPLINA (Código),
FOREIGN KEY(Matrícula) REFERENCES ALUNO (Matrícula)
)

